{
  "consumer": {
    "name": "my-sport-client"
  },
  "provider": {
    "name": "match-api"
  },
  "interactions": [
    {
      "description": "get all available matches",
      "request": {
        "method": "GET",
        "path": "/rest/api/v1/match/all"
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=UTF-8"
        },
        "body": [
          {
            "id": 1,
            "ownerId": 1,
            "startDate": null,
            "location": "location",
            "title": "title",
            "description": "description",
            "numberOfPlayers": 10,
            "paticipantIds": []
          }
        ],
        "matchingRules": {
          "body": {
            "$.name": {
              "matchers": [
                {
                  "match": "type"
                }
              ],
              "combine": "AND"
            }
          },
          "header": {
            "Content-Type": {
              "matchers": [
                {
                  "match": "regex",
                  "regex": "application/json;\\s?charset=(utf|UTF)-8"
                }
              ],
              "combine": "AND"
            }
          }
        }
      },
      "providerStates": [
        {
          "name": "exist football have id \u003d 123"
        }
      ]
    },
    {
      "description": "user request to join a match",
      "request": {
        "method": "PUT",
        "path": "/rest/api/v1/match/join",
        "requestParam": {
          "id": "123"
        }
      },
      "response": {
        "status": 202
      },
      "providerStates": [
        {
          "name": "user request to join an existing match have id \u003d 123"
        }
      ]
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "3.0.0"
    },
    "pact-jvm": {
      "version": "4.0.5"
    }
  }
}